							<TextBlock Text="Open Source"
									   FontSize="20"
									   Name="OpenSourceText"
									   FontWeight="500"
									   Foreground="White"
									   Padding="145,20,0,90"/>

							<Border CornerRadius="40"
									Width="240"
									Height="60"
									Name="BoxAboutIcons"
									Margin="20,0,0,0"
									Background="#252525">	
								
								<StackPanel Orientation="Horizontal">
									   <!--botao Github-->
										<Button Background="Transparent" CornerRadius="35"
												Padding="10,0,0,0" BorderThickness=""
